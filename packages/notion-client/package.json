{
  "name": "notion-client",
  "version": "6.15.17",
  "type": "module",
  "description": "Robust TypeScript client for the unofficial Notion API.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:ScriptAddicts/react-notion-x.git",
    "directory": "packages/notion-client"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/scriptaddicts/"
  },
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "got": "^11.8.1",
    "notion-types": "^6.15.17",
    "notion-utils": "^6.15.17",
    "p-map": "^5.3.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  },
  "gitHead": "9ab6a40731830f10a2ddb6fb2dfe632576cbdedb"
}
