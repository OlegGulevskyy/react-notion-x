{
  "name": "@scriptaddicts/notion-utils",
  "version": "6.15.19",
  "type": "module",
  "description": "Useful utilities for working with Notion data. Isomorphic.",
  "repository": {
    "type": "git",
    "url": "git@github.com:ScriptAddicts/react-notion-x.git",
    "directory": "packages/notion-utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/scriptaddicts/"
  },
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "is-url-superb": "^6.1.0",
    "mem": "^9.0.2",
    "normalize-url": "^7.0.3",
    "notion-types": "^6.15.18",
    "p-queue": "^7.2.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  },
  "gitHead": "cd84c45b7e89750a184d0ee89efacd51f27dd1ec"
}
